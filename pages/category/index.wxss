page {
  width: 100vw;
  height: 100vh;
}
.category {
  width: 100%;
  height: 100%;
}
.category .cates {
  display: flex;
  height: calc(100vh - 90rpx);
}
.category .cates y-menu {
  flex: 3;
  background-color: #F6F6F6;
  color: #9D9D9D;
}
.category .cates y-content {
  flex: 8;
  padding-left: 30rpx;
}
